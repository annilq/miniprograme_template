var getType = function (type) {
  switch (type) {
    case 1:
      return '文字';
    case 2:
      return '图片';
    case 3:
      return '语音';
    case 4:
      return '视频';
  }
};
var getStateText = function(treatmentStatus){
  switch (treatmentStatus) {
    case 0:
      return '全部状态';
    case 1:
      return '未接诊';
    // case 2:
    //   return '正在接诊';
    case 3:
      return '已接诊';
  }
};
module.exports = {
  getType: getType,
  getStateText:getStateText
}